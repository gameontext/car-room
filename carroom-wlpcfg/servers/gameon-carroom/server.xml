<server description="Game On! SimpleRoom Service">

    <!-- Enable features -->
    <featureManager>
		<feature>websocket-1.1</feature>
        <feature>jsonp-1.0</feature>
        <feature>jndi-1.0</feature>
    </featureManager>

    <!-- To access this server from a remote client add a host attribute to the following element, e.g. host="*" -->
    <httpEndpoint host="*" httpPort="9080"  id="defaultHttpEndpoint"/>

	<logging traceSpecification="*=info"/>

	<!-- This is required to prevent the web apps from being lazily loaded -->
	<webContainer deferServletLoad="false"/>

    <applicationMonitor dropinsEnabled="false"/>
    
    <jndiEntry jndiName="imageSvcUrl" value="http://192.168.0.103:8000/user"/>
    <!--
    <jndiEntry jndiName="carSvcUrl" value="ws://192.168.0.103:5060/cars/carRoom"/>
     -->
    <jndiEntry jndiName="mapSvcUrl" value="http://192.168.0.103:6080/map/v1/sites"/>
    <jndiEntry jndiName="devKey" value="nxsXJdYYSAKWuyIoQ9zFakefLvL04Ullgx+DihgEZz0="/>
    <jndiEntry jndiName="carEndPoint" value="ws://192.168.0.100:9080/LibertyCar/control"/>

    <webApplication id="carroom-app" location="carroom-app.war" name="carroom-app"/>
</server>
